// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
//Para efetuar as mudan√ßas no banco de dados, execute o comando: npx prisma migrate dev
//Para visualizar o banco de dados, execute o comando: npx prisma studio
model Contratos {
  id          String   @id @default(cuid())
  nome        String
  objeto      Int
  dataInicio  DateTime
  dataFim     DateTime
  valor       Float
  numProcesso String
  numContrato String
  nomeEmpresa String
  CNPJ        String
}
